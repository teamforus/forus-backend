{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0.2",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "astrotomic/laravel-translatable": "^11.9.1",
        "aws/aws-sdk-php": "3.191.2",
        "azuyalabs/yasumi": "^2.5",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-ide-helper": "^2.7",
        "bunq/sdk_php": "^1.14.18",
        "dusterio/laravel-aws-worker": "^0.1",
        "eduardokum/laravel-mail-auto-embed": "^2.0",
        "fruitcake/laravel-cors": "^2.0",
        "geoip2/geoip2": "^2.10.0",
        "graham-campbell/markdown": "14.0.x-dev",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.5",
        "itsgoingd/clockwork": "^5.0",
        "kalnoy/nestedset": "^6.0",
        "laravel-notification-channels/apn": "^4.0",
        "laravel-notification-channels/fcm": "^2.4",
        "laravel/framework": "^9.11",
        "laravel/helpers": "^1.2",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "predis/predis": "^1.1",
        "ralouphie/getallheaders": "2.0.5",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "twilio/sdk": "^6.8.0",
        "whichbrowser/parser": "^2.0.42",
        "zoon/commonmark-ext-youtube-iframe": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.25",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
