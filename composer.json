{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-zip": "*",
        "astrotomic/laravel-translatable": "^11.9.1",
        "aws/aws-sdk-php": "3.288.1",
        "barryvdh/laravel-dompdf": "^3.0",
        "barryvdh/laravel-ide-helper": "^3.1",
        "bunq/sdk_php": "1.27.17.30",
        "dusterio/laravel-aws-worker": "^1.0",
        "eduardokum/laravel-mail-auto-embed": "^2.0",
        "fakerphp/faker": "^1.20",
        "geoip2/geoip2": "^3.1.0",
        "graham-campbell/markdown": "^16.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^3.9",
        "itsgoingd/clockwork": "^5.0",
        "kalnoy/nestedset": "^6.0",
        "kreait/laravel-firebase": "^5.9",
        "laravel-notification-channels/apn": "^5.4",
        "laravel-notification-channels/fcm": "^4.3",
        "laravel/framework": "^11.0",
        "laravel/helpers": "^1.2",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mews/purifier": "^3.4",
        "mollie/mollie-api-php": "^2.0",
        "mollie/oauth2-mollie-php": "^2.7",
        "onelogin/php-saml": "^4.2",
        "pragmarx/google2fa": "^8.0",
        "predis/predis": "^2.3",
        "ralouphie/getallheaders": "^3.0",
        "simplesamlphp/saml2": "^4.6.3",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "twilio/sdk": "^8.3.9",
        "whichbrowser/parser": "^2.0.42",
        "zoon/commonmark-ext-youtube-iframe": "^2.0"
    },
    "require-dev": {
        "brianium/paratest": "^7.4",
        "laravel/dusk": "^8.2",
        "laravel/pint": "^1.20",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0",
        "vimeo/psalm": "^6.2"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "SimpleSAML\\": "app/Services/SAML2Service/SimpleSAML",
            "Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
